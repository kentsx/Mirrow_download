// Custom styles.

.divbox{ 
	width: 30%;
    height: 150px;
	position:relative;
	background:;
	cursor:pointer;
	display: inline-block;
	overflow: hidden;
	margin-top: 2% ;
    margin-left: 2.5%;
 }
.divbox .bmbox{ 
  width:100%; 
  height:30px; 
  background:black; 
  position:absolute; 
  bottom:0px; 
  left:0px; 
  display:none;
  font:12px/30px "arial"; 
  opacity: 0.9; 
  color: #fff; 
  background: rgba(0,0,0,0.7)
  text-align:center;
 }
.divbox:hover .bmbox{
  width:100%; 
  height:30px; 
  background:black; 
  position:absolute; 
  bottom:0px; 
  left:0px; 
  display:block;
  font:12px/30px "arial"; 
  opacity: 0.9; 
  color: #fff; 
  background: rgba(0,0,0,0.7)
  text-align:center;
 }
 
